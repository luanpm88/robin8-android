<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    android:id="@+id/ll_all"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="190dp"
    android:background="@color/white_custom">

    <com.robin8.rb.ui.widget.myprogress.RoundIndicatorView
        android:id="@+id/view_score_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        app:back_color="@color/yellow_ffc130"
        app:back_width="5dp"
        app:front_color1="@color/yellow_ffc701"
        app:front_color2="@color/yellow_ffc701"
        app:front_color3="@color/yellow_ffc701"
        app:front_width="5dp"
        app:is_need_content="false"
        app:is_need_content_two="true"
        app:is_need_title="false"
        app:is_need_unit="false"
        app:max_value="1000"
        app:start_engle="135"
        app:total_engle="270"
        app:need="9"
        />

    <com.robin8.rb.ui.widget.CircleImageView
        android:id="@+id/img_user_photo"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="35dp"
        android:background="@mipmap/icon_user_default"/>



</RelativeLayout>